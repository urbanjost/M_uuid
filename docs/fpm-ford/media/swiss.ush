set r=10
page -50 50 -50 50
makeobj 1234  # URBAN ART
linewidth 150
circleprecision 100
color WHITE
##############
polyfill TRUE
circle 0 35 2
polyfill FALSE
##############
# swiss cross
move2 0 -35-2
draw2 0 -35+2
move2 -2 -35
draw2 2 -35
##############
# case
move2 0-r -35
uarc 0 -35 180
draw2 0+r 35
uarc 0 35 180
draw2 0-r -35
##############
a=30
move2  0+r*cosd(a) 35+r*sind(a)
rdraw2 40*cosd(-45) 40*sind(-45)
rdraw2 18*cosd(-65) 18*sind(-65)
uarc 40 35 -24
draw2 0+r 20
##############
move2 0+r -35+3
draw2 0+4*r -35+3
draw2 0+4*r+3 -35
draw2 0+4*r -35-3
draw2 0+r -35-3
move2 0+4*r+3 -35
draw2 0+4*r -35
##############
invokeobj -50 -60 0 0.10 0.10 1 0 0 0 saw
closeobj
################################################
rm -f swiss.p6
hcopy p6 -obj 1234 -f swiss.p6 -sz 640 640
