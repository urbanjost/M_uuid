callobj 1234
vi framework.ush
# ==> read
sz 640 640
set TRUE=1 FALSE=0
set w=49 h=52 over=7 attic=14
################################################
genobj outerbrace
makeobj outerbrace
# horizontal
move2 left h/3
draw2 0-outer h/3
move2 right h/3
draw2 0+outer h/3
closeobj outerbrace
################################################
genobj innerbrace
makeobj innerbrace
# horizontal
move2 0-outer h/3
draw2 0-inner h/3
move2 0+outer h/3
draw2 0+inner h/3
closeobj innerbrace
################################################
genobj frame
makeobj frame
linewidth 300
set left=0-w/2 right=0+w/2
# outer box
move2 left h/2
draw2 right h/2
draw2 right 0-h/2
draw2 left 0-h/2
draw2 left h/2
# roof
move2 left-over  h/2
draw2 right+over h/2
draw2 0          h/2+attic
draw2 left-over  h/2
linewidth 200
move2 0 h/2
draw2 0+w/3 attic/2+h/2
move2 0 h/2
draw2 0-w/3 attic/2+h/2
move2 0 h/2
draw2 0 h/2+attic
# inner
set outer=w/3 inner=w/7
# vertical
move2 0-w/3 0-h/2
draw2 0-w/3 0+h/2
move2 0+w/3 0-h/2
draw2 0+w/3 0+h/2
move2 0-w/7 0-h/2
draw2 0-w/7 0+h/2
move2 0+w/7 0-h/2
draw2 0+w/7 0+h/2
invokeobj 0 0       0 1 1 1 0 0 0 outerbrace
invokeobj 0 0-h/4   0 1 1 1 0 0 0 outerbrace
invokeobj 0 0-2*h/4 0 1 1 1 0 0 0 outerbrace
set move=h/8
invokeobj 0 0-move  0 1 1 1 0 0 0 innerbrace
invokeobj 0 0-h/4-move   0 1 1 1 0 0 0 innerbrace
invokeobj 0 0-2*h/4-move 0 1 1 1 0 0 0 innerbrace
linewidth 400
move2 0-inner 0
draw2 0+inner 0
polyfill TRUE
color 0
makepoly
move2 0-inner 0
draw2 0+inner 0
draw2 0+inner 0-h/2
draw2 0-inner 0-h/2
draw2 0-inner 0
closepoly
color 7
linewidth 100
circle 0-0.8*inner 0-h/4 w/20
polyfill FALSE
closeobj
################################################
page -50 50 -50 50
color 7
makeobj 1234  # URBAN ART
circleprecision 60
linewidth 200
color 3
polyfill TRUE
circle 0 0 50
polyfill FALSE
color 7
linewidth 300
invokeobj 0 0-8 0 1 1 1 0 0 0 frame
closeobj 1234
rm -f framework.p6 framework.gif framework_long.gif
hcopy p6 -obj 1234 -f framework.p6 -sz 640 640
